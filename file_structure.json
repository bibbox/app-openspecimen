{
    "makefolders":[
        "var/app-config",
        "var/app-data",
        "images/openspecimen",
        "images/openspecimen/configs",
        "images/openspecimen/scripts"
    ],
    "copyfiles":[
        {
            "source":"config/database/db.conf",
            "destination":"var/app-config/db.conf"
        },
        {
            "source":"config/app-data/db.data",
            "destination":"var/config/app-data/db.data"
        },
        {
            "source":"image/openspecimen/Dockerfile",
            "destination":"image/openspecimen/Dockerfile"
        },
        {
            "source":"image/openspecimen/configs/build.properties",
            "destination":"image/openspecimen/configs/build.properties"
        },
        {
            "source":"image/openspecimen/configs/context.xml",
            "destination":"image/openspecimen/configs/context.xml"
        },
        {
            "source":"image/openspecimen/configs/openspecimen.cnf",
            "destination":"image/openspecimen/configs/openspecimen.cnf"
        },
        {
            "source":"image/openspecimen/configs/openspecimen.properties",
            "destination":"image/openspecimen/configs/openspecimen.properties"
        },
        {
            "source":"image/openspecimen/configs/package.json",
            "destination":"image/openspecimen/configs/package.json"
        },
        {
            "source":"image/openspecimen/configs/tomcat-users.xml",
            "destination":"image/openspecimen/configs/tomcat-users.xml"
        },
        {
            "source":"image/openspecimen/scripts/adduser.jar",
            "destination":"image/openspecimen/scripts/adduser.jar"
        },
        {
            "source":"image/openspecimen/scripts/defaultvar.sh",
            "destination":"image/openspecimen/scripts/defaultvar.sh"
        },
        {
            "source":"image/openspecimen/scripts/entrypoint.sh",
            "destination":"image/openspecimen/scripts/entrypoint.sh"
        },
        {
            "source":"image/openspecimen/scripts/sqlproperties.properties",
            "destination":"image/openspecimen/scripts/sqlporperties.properties"
        },
        {
            "source":"image/openspecimen/scripts/waitforit.sh",
            "destination":"image/openspecimen/scripts/waitforit.sh"
        }
    ]
}
    