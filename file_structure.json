{
    "makefolders":[
        "var/app-config",
        "var/app-data",
        "images/imagename"
    ],
    "copyfiles":[
        {
            "source":"config/database/db.conf",
            "destination":"var/app-config/db.conf"
        },
        {
            "source":"config/app-data/db.data",
            "destination":"var/config/app-data/db.data"
        },
        {
            "source":"image/imagename/*",
            "destination":"image/imagename/*"
        }
    ]
}
    